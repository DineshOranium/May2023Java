package TestScripts;

public class TypeConversionAndCasting {

	public static void main(String[] args) {

//		Type Conversion = Lower Data type -> Higher data type
//		Type casting = Higher data type -> lower data type
//		

		byte b = 10; // 1

		short s; // 2

		int m; // 4

		long n = 80; // 8
		
		
		s = b;
		
		System.out.println(s);
		
		
		m = s;
		
		System.out.println(m);
		
		
		int x = 100;
		
		byte b1 =(byte) x;
		
		System.out.println(b1);
		
		
		double d = 100.57;
		
		float f = (float)d;
		
		System.out.println(f);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
